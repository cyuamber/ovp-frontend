    "@babel/preset-env": "^7.5.5",
    "ant-design-vue": "^1.4.4",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "faker": "^4.1.0",
    "highcharts": "^7.2.1",
    "highcharts-vue": "^1.3.5",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.24.0",
    "sockjs-client": "^1.4.0",
    "stompjs": "^2.3.3",
    "vue": "^2.6.10",
    "vue-i18n": "^8.14.1",
    "vue-router": "^3.0.7",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "ghooks": "^2.0.4",
    "json-server": "^0.15.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "validate-commit-msg": "^2.14.0",
