    "core-js": "^2.6.11",
    "faker": "^4.1.0",
    "highcharts": "^7.2.1",
    "highcharts-vue": "^1.3.5",
    "mockjs": "^1.1.0",
    "moment": "^2.24.0",
    "reqwest": "^2.0.5",
    "sockjs-client": "^1.4.0",
    "stompjs": "^2.3.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.16.0",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
