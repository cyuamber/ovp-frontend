{
  "name": "ovp-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "watch": "vue-cli-service build --watch",
    "lint": "vue-cli-service lint",
    "mockconfig": "nodemon ./src/mock/server.js",
    "mock": "npm run mockconfig | npm run start",
    "dll": "webpack -p --progress --config ./build/webpack.dll.conf.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@vue/cli": "^4.4.1",
    "ant-design-vue": "^1.5.3",
    "axios": "^0.21.1",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "faker": "^4.1.0",
    "highcharts": "^8.1.1",
    "highcharts-vue": "^1.3.5",
    "husky": "^4.3.7",
    "mini-css-extract-plugin": "^0.9.0",
    "mockjs": "^1.1.0",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "reqwest": "^2.0.5",
    "sockjs-client": "^1.4.0",
    "stompjs": "^2.3.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "ghooks": "^2.0.4",
    "json-server": "^0.16.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^3.0.1",
    "thread-loader": "^2.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "validate-commit-msg": "^2.14.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11",
    "webpack-parallel-uglify-plugin": "^1.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "description": "This project is the frontend part of OVP project.",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyuamber/ovp-frontend.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cyuamber/ovp-frontend/issues"
  },
  "homepage": "https://github.com/cyuamber/ovp-frontend#readme"
}
